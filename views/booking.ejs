<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Mes Réservations</title>
  <link href="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/css/bootstrap.min.css" rel="stylesheet">
</head>
<body>
  <%- include('partials/nav') %>
  <div class="container mt-5">
    <h1 class="text-center">Mes Réservations</h1>
    <div class="row">
      <% bookings.forEach(booking => { %>
        <div class="col-md-4">
          <div class="card mb-4">
            <div class="card-body">
              <h5 class="card-title">Réservation pour : <%= booking.Housing.title %></h5>
              <p class="card-text"><strong>Date de début :</strong> <%= booking.startDate %></p>
              <p class="card-text"><strong>Date de fin :</strong> <%= booking.endDate %></p>
              <p class="card-text"><strong>Statut :</strong> <%= booking.status %></p>
            </div>
          </div>
        </div>
      <% }) %>
    </div>
  </div>
  <%- include('partials/footer') %>
  <script src="https://code.jquery.com/jquery-3.5.1.slim.min.js"></script>
  <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.9.2/dist/umd/popper.min.js"></script>
  <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.5.2/js/bootstrap.min.js"></script>
</body>
</html>

